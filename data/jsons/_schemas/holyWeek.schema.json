                                "noProstrations": {
                                  "type": "boolean"
                                },
                                "eshlil": {
                                  "type": "boolean"
                                },
                                "season": {
                                  "type": "string"
                                },
                                "nonTraditionalPascha": {
                                  "type": "boolean"
                                },
                                "firstVerse": {
                                  "type": "string"
                                }
